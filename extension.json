{
	"name": "GPMLConverter",
	"version": "0.01",
	"author": [
		"[https://wikipathways.org WikiPathways team]"
	],
	"url": "https://www.wikipathways.org",
	"descriptionmsg": "wp-gpml-desc",
	"license-name": "Apache-2.0",
	"type": "other",
	"config": {
		"_prefix": "wpi",
		"gpml2pvjsonPath": "node_modules/.bin/gpml2pvjson",
		"bridgedbPath": "node_modules/.bin/bridgedb",
		"jqPath": "/usr/bin/jq",
		"pvjsPath": "node_modules/.bin/pvjs"
	},
	"requires": {
		"MediaWiki": ">= 1.27.0"
	},
	"ResourceFileModulePaths": {
		"localBasePath": "node_modules",
		"remoteExtPath": "GPMLConverter/node_modules"
	},
	"ResourceModules": {
		"wpi.pvjs": {
			"scripts": [
				"@wikipathways/pvjs/dist/pvjs.js"
			]
		}
	},
	"AutoloadClasses": {
		"WikiPathways\\GPML\\Converter": "src/Converter.php"
	}
}
